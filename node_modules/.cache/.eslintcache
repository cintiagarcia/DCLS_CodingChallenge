          this.props.history.push(\"/phones\");\n            })\n            .catch ((err) => {\n                console.log(err);\n            })\n    };\n\n    componentDidMount(){\n       this.getPhoneData();\n    }\n\n    render () {\n    \n    if(!this.state.phone) return <Spinner animation=\"border\" role=\"status\"> <span className=\"visually-hidden\">Loading...</span> </Spinner>\n    \n    return(\n\n        <div className=\"phoneDetails\" >\n\n            <Card className=\"phoneCard\" style={{ width: '32rem' }}>\n\n                <CardBody>\n                    <img\n                        className=\"card-img-top img-responsive\"\n                        src={this.state.phone.imageFileName}\n                        alt=\"Card image cap\">\n                    </img>\n                </CardBody>\n\n                <CardBody >\n                    <CardTitle className=\"cardTitle text-white mb-3 p-2 \">{this.state.phone.name}</CardTitle>\n                    <CardText>\n                    {this.state.phone.description}\n                    </CardText>\n                </CardBody>\n                \n                <ListGroup className=\"list-group-flush\">\n                    <h1>{this.state.phone.manufacturer}</h1>\n                    <h4>{this.state.phone.color}</h4>\n                    <h4>{this.state.phone.color}</h4>\n                    <h4>{this.state.phone.price}</h4>\n                    <h4>{this.state.phone.screen}</h4>\n                    <h4>{this.state.phone.processor}</h4>\n                    <h4>{this.state.phone.ram}</h4>\n                </ListGroup>\n                <CardBody>\n                    <Link to={'/phone'}>\n                        <button\n                        className=\"buttonPhone btn btn-block my-4\"\n                        >\n                        Back to phones\n                        </button>\n                    </Link>\n                    <button \n                        className=\"buttonPhone btn btn-block my-4\"\n                        onClick= {this.deletePhone} >\n                        Delete Phone\n                    </button>\n                </CardBody>\n            </Card>\n        \n        </div>\n    )\n\n    }\n\n}\n\nexport default PhoneDetails; \n\n\n\n\n","/Users/cintia/Desktop/dcls/dcls_challenge/client/src/components/Phone.js",[],"/Users/cintia/Desktop/dcls/dcls_challenge/client/src/components/AddPhone.js",[],{"ruleId":"55","severity":1,"message":"56","line":1,"column":27,"nodeType":"57","messageId":"58","endLine":1,"endColumn":36},{"ruleId":"55","severity":1,"message":"59","line":8,"column":24,"nodeType":"57","messageId":"58","endLine":8,"endColumn":28},{"ruleId":"55","severity":1,"message":"60","line":8,"column":30,"nodeType":"57","messageId":"58","endLine":8,"endColumn":43},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"55","severity":1,"message":"65","line":4,"column":5,"nodeType":"57","messageId":"58","endLine":4,"endColumn":17},{"ruleId":"55","severity":1,"message":"66","line":5,"column":5,"nodeType":"57","messageId":"58","endLine":5,"endColumn":12},{"ruleId":"55","severity":1,"message":"67","line":9,"column":5,"nodeType":"57","messageId":"58","endLine":9,"endColumn":21},{"ruleId":"55","severity":1,"message":"68","line":11,"column":5,"nodeType":"57","messageId":"58","endLine":11,"endColumn":16},{"ruleId":"55","severity":1,"message":"59","line":14,"column":10,"nodeType":"57","messageId":"58","endLine":14,"endColumn":14},{"ruleId":"69","severity":1,"message":"70","line":22,"column":13,"nodeType":"71","endLine":22,"endColumn":46},{"ruleId":"55","severity":1,"message":"56","line":1,"column":16,"nodeType":"57","messageId":"58","endLine":1,"endColumn":25},{"ruleId":"55","severity":1,"message":"72","line":5,"column":11,"nodeType":"57","messageId":"58","endLine":5,"endColumn":18},{"ruleId":"55","severity":1,"message":"73","line":6,"column":16,"nodeType":"57","messageId":"58","endLine":6,"endColumn":28},{"ruleId